 labels);\n    },\n    assignTeam: function assignTeam(team) {\n      this.$emit('assign-team', team);\n    },\n    resolveConversations: function resolveConversations() {\n      this.$emit('resolve-conversations');\n    },\n    toggleUpdateActions: function toggleUpdateActions() {\n      this.showUpdateActions = !this.showUpdateActions;\n    },\n    toggleLabelActions: function toggleLabelActions() {\n      this.showLabelActions = !this.showLabelActions;\n    },\n    toggleAgentList: function toggleAgentList() {\n      this.showAgentsList = !this.showAgentsList;\n    },\n    toggleTeamsList: function toggleTeamsList() {\n      this.showTeamsList = !this.showTeamsList;\n    }\n  }\n};","map":{"version":3,"sources":["Index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwGA,OAAA,aAAA,MAAA,qBAAA;AACA,OAAA,aAAA,MAAA,qBAAA;AACA,OAAA,YAAA,MAAA,oBAAA;AACA,OAAA,WAAA,MAAA,mBAAA;AACA,eAAA;AACA,EAAA,UAAA,EAAA;AACA,IAAA,aAAA,EAAA,aADA;AAEA,IAAA,aAAA,EAAA,aAFA;AAGA,IAAA,YAAA,EAAA,YAHA;AAIA,IAAA,WAAA,EAAA;AAJA,GADA;AAOA,EAAA,KAAA,EAAA;AACA,IAAA,aAAA,EAAA;AACA,MAAA,IAAA,EAAA,KADA;AAEA,MAAA,OAAA,EAAA;AAAA,eAAA,EAAA;AAAA;AAFA,KADA;AAKA,IAAA,wBAAA,EAAA;AACA,MAAA,IAAA,EAAA,OADA;AAEA,MAAA,OAAA,EAAA;AAFA,KALA;AASA,IAAA,eAAA,EAAA;AACA,MAAA,IAAA,EAAA,KADA;AAEA,MAAA,OAAA,EAAA;AAAA,eAAA,EAAA;AAAA;AAFA,KATA;AAaA,IAAA,cAAA,EAAA;AACA,MAAA,IAAA,EAAA,OADA;AAEA,MAAA,OAAA,EAAA;AAFA,KAbA;AAiBA,IAAA,kBAAA,EAAA;AACA,MAAA,IAAA,EAAA,OADA;AAEA,MAAA,OAAA,EAAA;AAFA,KAjBA;AAqBA,IAAA,iBAAA,EAAA;AACA,MAAA,IAAA,EAAA,OADA;AAEA,MAAA,OAAA,EAAA;AAFA;AArBA,GAPA;AAiCA,EAAA,IAjCA,kBAiCA;AACA,WAAA;AACA,MAAA,cAAA,EAAA,KADA;AAEA,MAAA,iBAAA,EAAA,KAFA;AAGA,MAAA,gBAAA,EAAA,KAHA;AAIA,MAAA,aAAA,EAAA,KAJA;AAKA,MAAA,gBAAA,EAAA;AALA,KAAA;AAOA,GAzCA;AA0CA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,qBACA,CADA,EACA;AACA,WAAA,KAAA,CAAA,0BAAA,EAAA,CAAA,CAAA,MAAA,CAAA,OAAA;AACA,KAHA;AAIA,IAAA,MAJA,kBAIA,KAJA,EAIA;AACA,WAAA,KAAA,CAAA,cAAA,EAAA,KAAA;AACA,KANA;AA